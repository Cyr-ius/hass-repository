{
  "name": "Dnsmasq advanced",
  "version": "1.7.4",
  "slug": "dnsmasqadvanced",
  "description": "A simple DNS and DHCP server",
  "url": "https://github.com/cyr-ius/hass-repository/",
  "arch": [
    "armhf",
    "armv7",
    "aarch64"
  ],
  "advanced": true,
  "startup": "system",
  "boot": "auto",
  "ports": {
    "53/tcp": 53,
    "53/udp": 53,
    "547/tcp": 547,
    "547/udp": 547
  },
  "host_network": true,
  "full_access": true,
  "privileges": ["NET_ADMIN"],
  "apparmor": true,
  "options": {
    "debug": false,
    "logqueries": false,
    "domain":[],
    "defaults": [
      "8.8.8.8",
      "8.8.4.4"
    ],
    "forwards": [],
    "hosts": [],
    "dhcprange": [],
    "dhcphost": [],
    "dhcpoption": [],
    "enablera": false,
    "raparam":""
  },
  "schema": {
    "debug": "bool",
    "logqueries": "bool",
    "domain": ["str"],  
    "defaults": ["str"],
    "forwards": [
      {
        "domain": "str",
        "server": "str"
      }
    ],
    "hosts": [
      {
        "host": "str",
        "ip": "str"
      }
    ],
    "dhcprange": [
    {
        "range": "str"
      }
    ],
    "dhcphost": [
      {
        "mac": "str",
        "value": "str"
      }
    ],
    "dhcpoption": [
      {
        "option": "str",
        "value": "str"
      }
    ],
    "enablera": "bool",    
    "raparam": "str"    
  }
}
