{
  "name": "Dnsmasq advanced",
  "version": "1.5.4",
  "slug": "dnsmasqadvanced",
  "description": "A simple DNS and DHCP server",
  "url": "https://github.com/cyr-ius/hass-repository/",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "ports": {
    "53/tcp": 53,
    "53/udp": 53
  },
  "host_network": true,
  "full_access": true,
  "options": {
    "domain":[],
    "defaults": [
      "8.8.8.8",
      "8.8.4.4"
    ],
    "forwards": [],
    "hosts": [],
    "dhcprange": [],
    "dhcphost": [],
    "dhcpoption": []
  },
  "schema": {
    "domain": [
      "str"
    ],  
    "defaults": [
      "str"
    ],
    "forwards": [
      {
        "domain": "str",
        "server": "str"
      }
    ],
    "hosts": [
      {
        "host": "str",
        "ip": "str"
      }
    ],
    "dhcprange": [
      {
        "start": "str",
        "end": "str",
        "mask": "str",
        "delay": "str"
      }
    ],
    "dhcphost": [
      {
        "mac": "str",
        "ip": "str"
      }
    ],
    "dhcpoption": [
      {
        "item": "str",
        "ip": "str"
      }
    ]      
  }
}
